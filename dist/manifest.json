{
  "manifest_version": 3,
  "name": "Sungrow API Auth",
  "version": "1.0",
  "description": "Firefox addon for authorizing with Sungrow Developer API",
  "permissions": [
    "storage",
    "activeTab",
    "alarms",
    "identity"
  ],
  "host_permissions": [
    "https://*.isolarcloud.com/"
  ],
  "background": {
    "scripts": [
      "assets/background.ts-NzY5-YPe.js"
    ],
    "type": "module"
  },
  "action": {
    "default_title": "Sungrow API Auth",
    "default_icon": {
      "16": "icons/icon-16.svg",
      "32": "icons/icon-32.svg",
      "48": "icons/icon-48.svg",
      "128": "icons/icon-128.svg"
    },
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon-16.svg",
    "32": "icons/icon-32.svg",
    "48": "icons/icon-48.svg",
    "128": "icons/icon-128.svg"
  }
}
